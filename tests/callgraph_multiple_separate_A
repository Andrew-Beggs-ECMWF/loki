strict digraph {
	DRIVERB [color=black fillcolor=limegreen shape=box style=filled]
	KERNELB [color=black fillcolor=limegreen shape=box style=filled]
	COMPUTE_L1 [color=black fillcolor=limegreen shape=box style=filled]
	COMPUTE_L2 [color=black fillcolor=limegreen shape=box style=filled]
	DRIVERB -> KERNELB
	KERNELB -> COMPUTE_L1
	COMPUTE_L1 -> COMPUTE_L2
	EXT_DRIVER [color=black fillcolor=lightblue shape=box style=filled]
	KERNELB -> EXT_DRIVER
}
